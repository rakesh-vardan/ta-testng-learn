<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="All Test Suite">
    <groups>
        <run>
            <include name="SmokeTest"/>
            <include name="SanityTest"/>
        </run>
